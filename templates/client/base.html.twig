<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <style>
        :root{

        }
    </style>
    {% block stylesheets %}
        <link rel="icon" type="image/png/webp" href="{{ asset('images/logo.webp') | imagine_filter('ma_vignette') }}"/>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
              integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
              crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
        <link rel="stylesheet" href="{{ asset('styles/image.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/app.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/profil.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/articlepage.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/navbar.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/create.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/audio.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/video.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/form.css') }}">
        <link rel="stylesheet" href="{{ asset('styles/about.css') }}">
    {% endblock %}

    {% block javascripts %}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
                crossorigin="anonymous"></script>
        <script src="{{ asset('js/hello_controller.js') }}"></script>
        {% block importmap %}
            {{ importmap('app') }}{% endblock %}
    {% endblock %}
</head>
<body>
{% include 'navbar.html.twig' %}
  {% block body %}{% endblock %}

</body>
</html>
